    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Numerals: with range of possible values</title>
                <respStmt>
                    <resp/>
                    <name/>
                </respStmt>
                <respStmt>
                    <resp/>
                    <name/>
                </respStmt>
            </titleStmt>
            <publicationStmt><p/></publicationStmt>
